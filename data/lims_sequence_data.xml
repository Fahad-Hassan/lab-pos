<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!--
            Sequences for lims
        -->


        
        <record id="seq_lims_sample_main" model="ir.sequence">
            <field name="name">Medical report</field>
            <field name="code">lims.sample.main</field>
            <field name="prefix">MRN/%(y)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_lims_sample" model="ir.sequence">
            <field name="name">Sample</field>
            <field name="code">lims.sample.preparation</field>
            <field name="prefix">SP/%(y)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_lims_analysis" model="ir.sequence">
            <field name="name">Analysis</field>
            <field name="code">lims.analysis</field>
            <field name="prefix">ANL/%(y)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
